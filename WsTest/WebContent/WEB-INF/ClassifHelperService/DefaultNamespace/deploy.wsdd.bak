<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ClassifHelperService WSDL service -->

  <service name="ClassifHelper" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="ClassifHelperService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="ClassifHelper"/>
      <parameter name="className" value="DefaultNamespace.ClassifHelperSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ClassifHelper"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="classificationresults" qname="operNS:classificationresults" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:classificationresultsReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:Inputarff" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="classificationresults"/>

  </service>
</deployment>
